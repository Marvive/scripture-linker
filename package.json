{
    "name": "scripture-linker",
    "version": "1.0.0",
    "description": "Scan notes for Bible references and link them to Logos Bible Software and Bolls Bible",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json"
    },
    "keywords": [
        "obsidian",
        "plugin",
        "bible",
        "scripture",
        "logos"
    ],
    "author": "Michael Marvive",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^16.11.6",
        "builtin-modules": "^3.3.0",
        "esbuild": "0.17.3",
        "obsidian": "latest",
        "tslib": "2.4.0",
        "typescript": "4.7.4"
    }
}